{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "relay": {
      "cache": false
    },
    "relay:web": {
      "cache": false,
      "dependsOn": ["relay"]
    }
  }
}
